class mahasiswa {
    private int npm;
    private String nama;
    public mahasiswa(int n, String nm){
      npm = n; nama = nm;
      System.out.println("Constructor: "+npm+" "+nama);
    }
    public String cetak(){
      return npm+" "+nama;
    }
}
